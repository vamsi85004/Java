import java.util.Scanner;
public class choicetocheckpalin
{
public static void main(String[] args)
{
String s1="MADAM";
String s2="";
int len=s1.length();
for(int i=len-1;i>=0;i--)
{
s2=s2+s1.charAt(i);
}
if(s1.equals(s2))
System.out.print("Palindrome");
else
System.out.print("Not palindrome");
}
}


OUTPUT:
ooad@ooad-Lenovo-S510:~/Desktop$ javac choicetocheckpalin.java
ooad@ooad-Lenovo-S510:~/Desktop$ java choicetocheckpalin
Palindrome
